    "@types/react-native": "^0.51.9",
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "jest": "18.1.0",
    "lodash.isequal": "^4.5.0",
    "lodash.uniqwith": "^4.5.0",
    "patch-package": "^5.0.0",
    "prop-types": "^15.6.0",
